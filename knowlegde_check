#convrt numbers to strings in codewars
def number_to_string(num):
    # Return a string of the number here!
    return str(num)

# VOWEL counter
def get_count(sentence):
    num_vowels = 0
    vowels = "aeiou"
    for x in sentence:
        if x in vowels:
            num_vowels += 1
    return num_vowels


#even or odd
def even_or_odd(number):
    return "Even" if number %2 == 0 else "Odd"